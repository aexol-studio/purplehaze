{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "noEmit": true,
    "baseUrl": "",
    "paths": {
      "https://unpkg.com/htm/preact/index.mjs?module": [
        ".purplehaze/typings/index.mjs?module/index.d.ts"
      ]
    }
  },
  "include": [
    "./pages/**/*.tsx",
    "./pages/**/*.jsx",
    "./pages/**/*.ts",
    "./pages/**/*.js",
    "./pages/purplehaze.d.ts"
  ]
}